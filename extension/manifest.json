{
  "name": "Vue Clicky",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Handy debugging tool for Vue. Right click a component to see key info in the console.",
  "homepage_url": "https://github.com/Herteby/vue-clicky",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": [
        "inject.js"
      ]
    }
  ]
}